{
  "name": "postcss-sanitize",
  "version": "0.0.9",
  "description": "PostCSS plugin that removes properties and values based on options (think like a CSS sanitizer)",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "sanitizer"
  ],
  "author": "Damien Erambert <damien@erambert.me>",
  "license": "MIT",
  "repository": "eramdam/postcss-sanitize",
  "bugs": {
    "url": "https://github.com/eramdam/postcss-sanitize/issues"
  },
  "homepage": "https://github.com/eramdam/postcss-sanitize",
  "dependencies": {
    "lodash.isregexp": "^4.0.1",
    "postcss-value-parser": "^4.1.0"
  },
  "peerDependencies": {
    "postcss": "^8.3.6"
  },
  "devDependencies": {
    "ava": "^3.15.0",
    "eslint": "^7.32.0",
    "eslint-config-postcss": "^4.0.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2",
    "postcss": "^8.3.6",
    "prettier": "^2.3.2"
  },
  "scripts": {
    "test": "ava && prettier --check *.js && eslint *.js",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix && prettier --write"
  }
}